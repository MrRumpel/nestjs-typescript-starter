{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "ng/next/(.*)": "<rootDir>/../libs/next/src/$1",
    "ng/next": "<rootDir>/../libs/next/src"
  }
}